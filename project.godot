; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Inside The Room"
run/main_scene="res://scenes/ui/main_menu.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="InsideTheRoom"
config/features=PackedStringArray("4.3", "Forward Plus")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/show_image=false
config/icon="res://assets/logo/robot_logo.png"

[autoload]

SaveManager="*res://scenes/autoload/save_manager.tscn"
GameEvents="*res://scenes/autoload/game_events.tscn"
AudioManager="*res://scenes/autoload/audio_manager.tscn"
LevelTransitionManager="*res://scenes/autoload/level_transition_manager.tscn"
LevelManager="*res://scenes/autoload/level_manager.tscn"
OptionsManager="*res://scenes/autoload/options_manager.tscn"

[debug]

gdscript/warnings/unassigned_variable=2
gdscript/warnings/unassigned_variable_op_assign=2
gdscript/warnings/shadowed_variable=2
gdscript/warnings/shadowed_variable_base_class=2
gdscript/warnings/shadowed_global_identifier=2
gdscript/warnings/untyped_declaration=2
gdscript/warnings/unsafe_property_access=1
gdscript/warnings/unsafe_method_access=1
gdscript/warnings/unsafe_cast=1
gdscript/warnings/unsafe_call_argument=1

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="canvas_items"
window/size/mode.release=4

[dotnet]

project/assembly_name="robot-vacuum"

[file_customization]

folder_colors={
"res://assets/": "purple",
"res://resources/": "yellow",
"res://scenes/": "green",
"res://scripts/": "blue"
}

[filesystem]

import/blender/enabled=false

[global_group]

is_garbage="object is garbage. must implement IGarbage"

